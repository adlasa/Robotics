package org.usfirst.frc.team1458.robot;

public class LED {
	private byte r;
	private byte g;
	private byte b;
	private boolean on = false;
	LED() {
		
	}
	public void set(byte r, byte g, byte b) {
		this.r = r;
		this.g = g;
		this.b = b;
	}
	public void turnOn() {
		
		on = true;
	}
	public void turnOff() {
		on = false;
	}

}
